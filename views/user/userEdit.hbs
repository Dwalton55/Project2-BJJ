<h1>Edit {{indivUser.name}}</h1>

<form action="/user/{{indivUser._id}}?_method=PUT" method="POST">
  <div>
    <label for="name">Change Name</label>
    <input type="text" name="name" value={{indivUser.name}} />
  </div>
  <div>
    <label for="coach">Change Coach</label>
    <input type="text" name="coach" value={{indivUser.coach}}>
  </div>
  <div>
    <label for="school">Change School</label>
    <input type="text" name="school" value={{indivUser.school}} />
  </div>
  
  <button type="submit">Update</button>
</form>

<button><a href="/user/{{indivUser._id}}">back</a></button>